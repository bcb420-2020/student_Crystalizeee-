  pages = {139-140},
  pmid = {19910308},
  timestamp = {2018-10-17T21:51:45.000+0200},
  title = {edgeR: a Bioconductor package for differential expression analysis of digital gene expression data},
  url = {https://www.ncbi.nlm.nih.gov/pubmed/19910308},
  volume = 26,
  year = 2010
}

